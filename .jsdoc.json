{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "sourceType": "module",
  "source": {
    "include": [
      "formulas",
      "grammar",
      "README.md",
      "docs/generated_diagrams.html"
    ],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "markdown": {
    "idInHeadings": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "useLongnameInNav": false,
    "showInheritedInNav": true
  },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "recurse": true,
    "verbose": true,
    "template": "./node_modules/docdash",
    "package": ""
  }
}
